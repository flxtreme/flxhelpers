{
  "name": "flxhelpers",
  "version": "1.0.7",
  "description": "Typescript/Javascript utilities/helpers functions",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "browser": "dist/index.client.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./client": {
      "import": "./dist/index.client.js",
      "require": "./dist/index.client.js"
    },
    "./helpers/*": {
      "import": "./dist/helpers/*.js",
      "require": "./dist/helpers/*.js"
    },
    "./utils/*": {
      "import": "./dist/utils/*.js",
      "require": "./dist/utils/*.js"
    },
    "./types/*": {
      "import": "./dist/types/*.js",
      "require": "./dist/types/*.js"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/flxtreme/flxhelpers.git"
  },
  "keywords": ["helpers", "utilities", "functions", "flxtremee"],
  "type": "module",
  "author": "flxtremee",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/flxtreme/flxhelpers/issues"
  },
  "homepage": "https://github.com/flxtreme/flxhelpers#readme",
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/lodash": "^4.17.20",
    "@types/node": "^24.8.1",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "bcrypt": "^6.0.0",
    "date-fns": "^4.1.0",
    "lodash": "^4.17.21"
  }
}
